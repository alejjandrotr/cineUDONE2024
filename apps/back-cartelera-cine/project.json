{
  "name": "back-cartelera-cine",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/back-cartelera-cine/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["apps/back-cartelera-cine"],
      "options": {
        "outputPath": "apps/back-cartelera-cine",
        "main": "back-cartelera-cine/src/main.ts",
        "tsConfig": "back-cartelera-cine/tsconfig.app.json",
        "assets": []
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        },
        "production": {
          "optimization": true,
          "sourceMap": false,
          "extractLicenses": true
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "back-cartelera-cine:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "back-cartelera-cine:build:development"
        },
        "production": {
          "buildTarget": "back-cartelera-cine:build:production"
        }
      }
    }
  }
}
